<Window x:Class="VehicleMakerUI.UI.InitialUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VehicleMakerUI"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" Height="310" Width="502" ResizeMode="NoResize">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0.829"/>
            <GradientStop Color="#FF556BE8" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Image HorizontalAlignment="Left" Height="100" Margin="577,386,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="btnStart" Content="만@들어버리기" HorizontalAlignment="Center" Height="31" Margin="0,199,0,0" VerticalAlignment="Top" Width="122" Background="#FFFD0606" Foreground="#FFE4FD00" Click="btnStart_Click"/>
        <Label x:Name="lbMain" Content="VehicleMaker" HorizontalAlignment="Center" Height="44" Margin="0,71,0,0" VerticalAlignment="Top" Width="158" Foreground="#FFFDFDFD" FontSize="22" FontWeight="DemiBold"/>
        <Label Content="탁성건"
       HorizontalAlignment="Left"
       VerticalAlignment="Top"
       Margin="148,147,0,0"
       FontSize="10" FontWeight="DemiBold" Foreground="#FFFDFDFD"/>

        <Label Content="김세진"
       HorizontalAlignment="Center"
       VerticalAlignment="Top"
       Margin="0,147,0,0"
       FontSize="10" FontWeight="DemiBold" Foreground="#FFFDFDFD"/>

        <Label Content="남정훈"
       HorizontalAlignment="Right"
       VerticalAlignment="Top"
       Margin="0,147,144,0"
       FontSize="10" FontWeight="DemiBold" Foreground="#FFFDFDFD"/>
    </Grid>
</Window>
